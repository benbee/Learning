#include <stdio.h>

int main(int argc, char *argv[])
{
    int i, j, m, n;

    for (i = 0; i < 5; ++i) {
	for (j =  5-i-1; j > 0 ; j--) {
	    printf(" ");
	}
	for (j = 0; j <= i; j++) {
	    printf("%c", j + 'A');
	}
	for (j = i; j > 0; j--) {
	    printf("%c", j + 'A');
	}
    }
    
    return 0;
}
